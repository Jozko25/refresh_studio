{
  "type": "webhook",
  "name": "refresh_clinic",
  "description": "REFRESH clinic booking assistant",
  "api_schema": {
    "url": "https://refreshstudio-production.up.railway.app/api/elevenlabs",
    "method": "POST",
    "request_body_schema": {
      "type": "object",
      "properties": [
        {
          "id": "tool_name",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "Tool to use: search_service",
          "required": true
        },
        {
          "id": "search_term", 
          "type": "string",
          "value_type": "llm_prompt",
          "description": "Service name to search for",
          "required": false
        }
      ]
    }
  }
}