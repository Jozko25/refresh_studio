{
  "type": "webhook", 
  "name": "refresh_test",
  "description": "REFRESH clinic booking system",
  "disable_interruptions": false,
  "force_pre_tool_speech": "auto",
  "assignments": [],
  "api_schema": {
    "url": "https://refreshstudio-production.up.railway.app/api/elevenlabs",
    "method": "POST",
    "path_params_schema": [],
    "query_params_schema": [],
    "request_body_schema": {
      "id": "body",
      "type": "object",
      "description": "Complete booking parameters",
      "properties": [
        {
          "id": "tool_name",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "The tool function to call: \"search_service\" to find services by name, \"find_soonest_slot\" to get earliest available appointment, \"get_services_overview\" to show popular services, \"get_opening_hours\" for clinic hours.",
          "dynamic_variable": "",
          "constant_value": "",
          "required": true
        },
        {
          "id": "search_term",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "Service name to search for. Extract customer's request for specific treatments like \"hydrafacial\", \"peeling\", \"laserová epilácia\", \"institut esthederm\", etc. Required only for search_service tool.",
          "dynamic_variable": "",
          "constant_value": "",
          "required": false
        },
        {
          "id": "service_id",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "Service ID from previous search results. Use the serviceId returned by search_service when customer selects a specific service. Required for find_soonest_slot tool.",
          "dynamic_variable": "",
          "constant_value": "",
          "required": false
        },
        {
          "id": "worker_id",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "Worker: -1 for any, 18204 for Janka, 30224 for Veronika",
          "dynamic_variable": "",
          "constant_value": "-1",
          "required": false
        },
        {
          "id": "date",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "Date in DD.MM.YYYY or YYYY-MM-DD format",
          "dynamic_variable": "",
          "constant_value": "",
          "required": false
        },
        {
          "id": "time",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "Time in HH:MM format (e.g. 14:30)",
          "dynamic_variable": "",
          "constant_value": "",
          "required": false
        }
      ],
      "required": false,
      "value_type": "llm_prompt"
    },
    "request_headers": [],
    "auth_connection": null
  },
  "response_timeout_secs": 20,
  "dynamic_variables": {
    "dynamic_variable_placeholders": {}
  }
}