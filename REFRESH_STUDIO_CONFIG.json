          "dynamic_variable": "",
          "constant_value": "",
          "required": false
        },
        {
          "id": "appointment_type",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "Typ služby: wellness_session, consultation, relaxation_treatment",
          "dynamic_variable": "",
          "constant_value": "",
