{
  "type": "webhook",
  "name": "refresh_studio_booking",
  "description": "Kompletný systém rezervácie termínov pre Refresh Studio s pokročilou validáciou",
  "disable_interruptions": false,
  "force_pre_tool_speech": "auto",
  "assignments": [],
  "api_schema": {
    "url": "https://refreshstudio-production.up.railway.app/api/booking/webhook/elevenlabs-unified",
    "method": "POST",
    "path_params_schema": [],
    "query_params_schema": [],
    "request_body_schema": {
      "id": "body",
      "type": "object",
      "description": "Refresh Studio booking system request",
      "properties": [
        {
          "id": "action",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "Typ akcie: get_available_times, get_soonest_available, get_more_slots, book_appointment, reschedule_appointment, cancel_appointment",
          "dynamic_variable": "",
          "constant_value": "",
          "required": true
        },
        {
          "id": "date",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "Dátum vo formáte DD.MM.YYYY alebo prirodzený jazyk (dnes, zajtra, pondelok)",
          "dynamic_variable": "",
          "constant_value": "",
          "required": false
        },
        {
          "id": "time",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "Čas vo formáte HH:MM",
          "dynamic_variable": "",
          "constant_value": "",
          "required": false
        },
        {
          "id": "date_time",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "ISO formát YYYY-MM-DDTHH:mm:ss pre validáciu",
          "dynamic_variable": "",
          "constant_value": "",
          "required": false
        },
        {
          "id": "phone",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "Slovenské telefónne číslo +421XXXXXXXXX (povinné pre rezerváciu/zrušenie)",
          "dynamic_variable": "",
          "constant_value": "",
          "required": false
        },
        {
          "id": "patient_name",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "Krstné meno zákazníka",
          "dynamic_variable": "",
          "constant_value": "",
          "required": false
        },
        {
          "id": "patient_surname",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "Priezvisko zákazníka",
          "dynamic_variable": "",
          "constant_value": "",
          "required": false
        },
        {
          "id": "full_patient_name",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "Celé meno zákazníka pre overenie (presunutie/zrušenie)",
          "dynamic_variable": "",
          "constant_value": "",
          "required": false
