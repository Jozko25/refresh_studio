{
  "type": "webhook",
  "name": "refresh_studio_booking",
  "description": "Refresh Studio booking system",
  "disable_interruptions": false,
  "force_pre_tool_speech": "auto",
  "assignments": [],
  "api_schema": {
    "url": "https://refreshstudio-production.up.railway.app/api/booking/webhook/elevenlabs-unified",
    "method": "POST",
    "path_params_schema": [],
    "query_params_schema": [],
    "request_body_schema": {
      "id": "body",
      "type": "object",
      "description": "Booking request",
      "properties": [
        {
          "id": "action",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "Action: get_available_times, get_soonest_available, book_appointment, cancel_appointment",
          "required": true
        },
        {
          "id": "date",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "Date in DD.MM.YYYY format",
          "required": false
        },
        {
          "id": "time",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "Time in HH:MM format",
          "required": false
        },
        {
          "id": "customer",
          "type": "object",
          "value_type": "llm_prompt",
          "description": "Customer data as JSON object with firstName, lastName, email, phone",
          "required": false
        },
        {
          "id": "phone",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "Phone number for cancellation",
          "required": false
        },
        {
          "id": "appointment_date",
          "type": "string",
          "value_type": "llm_prompt",
          "description": "Appointment date for cancellation",
          "required": false
        }
      ],
      "required": false,
      "value_type": "llm_prompt"
    },
    "request_headers": [
      {
        "name": "Content-Type",
        "value": "application/json"
      }
    ],
    "auth_connection": null
  },
  "response_timeout_secs": 30,
  "dynamic_variables": {
    "dynamic_variable_placeholders": {}
  }
}